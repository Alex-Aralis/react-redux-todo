{
  "parserOptions": {
    "ecmaVersion": 7,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "presets": ["react", "es2015"],
    "development": {
      "presets": ["react-hmre"]
    },
    "node": true,
    "commonjs": true,
    "shared-node-broswer": true
  },
  "rules": {
    "quotes": ["error", "single"]
  }
}
